{
  "name": "enterprise-rpa-agent",
  "version": "1.0.0",
  "description": "Enterprise RPA Agent for mass browser automation",
  "main": "dist-electron/electron/main.js",
  "private": true,
  "author": {
    "name": "Enterprise RPA"
  },
  "scripts": {
    "dev": "node scripts/dev.js",
    "dev:next": "next dev",
    "dev:electron": "wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .",
    "build": "node scripts/build.js",
    "build:next": "next build",
    "build:electron": "tsc --project tsconfig.electron.json",
    "start": "electron .",
    "package": "npm run build && electron-builder --win --dir",
    "dist": "npm run build && electron-builder",
    "dist:win": "npm run build && electron-builder --win",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "postinstall": "npx playwright install chromium",
    "debug:proxy": "npm run build:electron && node dist-electron/electron/scripts/debug-proxy.js",
    "setup:proxy": "npm run build:electron && node dist-electron/electron/scripts/setup-working-proxy.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-popover": "1.1.4",
    "@radix-ui/react-progress": "1.1.1",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slider": "1.2.2",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/react-tabs": "1.1.2",
    "@radix-ui/react-toast": "1.2.4",
    "@radix-ui/react-tooltip": "1.1.6",
    "better-sqlite3": "^11.0.0",
    "bezier-easing": "^2.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "dotenv": "^16.4.5",
    "fingerprint-generator": "^2.1.0",
    "fingerprint-injector": "^2.1.0",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.6",
    "jstat": "^1.9.6",
    "lucide-react": "^0.454.0",
    "next": "15.0.0",
    "next-themes": "^0.4.6",
    "playwright": "^1.45.0",
    "playwright-core": "^1.45.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "2.15.4",
    "socks-proxy-agent": "^8.0.5",
    "sonner": "^1.7.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.9",
    "@types/better-sqlite3": "^7.6.10",
    "@types/bezier-easing": "^2.1.3",
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^30.0.0",
    "electron-builder": "^24.13.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.0",
    "postcss": "^8.5.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.6.0",
    "wait-on": "^7.2.0"
  }
}
